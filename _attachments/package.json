{
  "name": "coconut-mobile",
  "version": "0.9.0",
  "scripts": {
    "start": "npm run watch & npm run livereload & npm run static",
    "watch": "./node_modules/watchify/bin/cmd.js -v -t coffeeify --debug --extension='.coffee' app/start.coffee -o bundle.js",
    "livereload": "./node_modules/livereloadify/cli.js ./",
    "watchAndTest": "./node_modules/chokidar-cli/index.js bundle.js -c mocha --compilers mocha --compilers coffee:coffee-script/register test/*.coffee",
    "static": "./node_modules/node-static/bin/cli.js --port 8089 ./"
  },
  "dependencies": {
    "Base64": "^0.3.0",
    "appcache-nanny": "^1.0.3",
    "awesomplete": "^1.0.0",
    "backbone": "^1.2.1",
    "backbone-pouch": "^1.4.0",
    "blob-util": "^1.1.2",
    "chokidar": "^1.2.0",
    "chokidar-cli": "^1.1.0",
    "coffee-script": "^1.9.3",
    "coffeeify": "^1.1.0",
    "crypto-pouch": "^1.1.2",
    "datatables": "^1.10.7",
    "file-saver": "^1.3.2",
    "form2js": "git://github.com/maxatwork/form2js",
    "jquery": "^2.1.4",
    "jquery.cookie": "^1.4.1",
    "js-cookie": "^2.0.0-pre",
    "jszip": "^3.0.0",
    "livereloadify": "^2.0.0",
    "lodash.flatten": "^3.0.2",
    "material-design-icons": "^2.0.0",
    "material-design-lite": "^1.3.0",
    "memorystream": "^0.3.1",
    "mochify": "^2.13.0",
    "moment": "^2.10.3",
    "node-static": "^0.7.7",
    "pouchdb": "*",
    "pouchdb-all-dbs": "^1.0.1",
    "pouchdb-replication-stream": "^1.2.6",
    "sha1": "^1.1.1",
    "uglify": "^0.1.5",
    "underscore": "^1.8.3",
    "underscore.string": "^3.2.2",
    "watchify": "^3.6.1"
  }
}
