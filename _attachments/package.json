{
  "name": "coconut-mobile",
  "version": "0.9.0",
  "scripts": {
    "start": "npm run watch & npm run livereload & npm run static & while sleep 2; do echo -n '.'; done",
    "watch": "./node_modules/watchify/bin/cmd.js --ignore-watch='**/node_modules/**' -v -t coffeeify --debug --extension='.coffee' app/start.coffee -x moment -x jquery -x backbone -x pouchdb-core -x pouchdb-adapter-http -x pouchdb-mapreduce -x pouchdb-upsert -x underscore -x tabulator-tables -o bundle.js",
    "livereload": "./node_modules/livereloadify/cli.js ./",
    "watchAndTest": "./node_modules/chokidar-cli/index.js bundle.js -c mocha --compilers mocha --compilers coffee:coffee-script/register test/*.coffee",
    "static": "./node_modules/node-static/bin/cli.js --host-address 0.0.0.0 --port 8089 ./",
    "buildBundle": "./node_modules/browserify/bin/cmd.js --verbose -t coffeeify --extension='.coffee' app/start.coffee -x moment -x jquery -x backbone -x pouchdb-core -x pouchdb-adapter-http -x pouchdb-find -x pouchdb-mapreduce -x pouchdb-replication -x pouchdb-upsert -x underscore -x tabulator-tables > bundle.js",
    "buildCss": "./buildCss.sh",
    "buildJsLibraries": "./buildJsLibraries.sh",
    "postinstall": "npm run buildBundle && npm run buildCss && npm run buildJsLibraries",
    "install-server": "npm install --no-optional --unsafe-perm"
  },
  "dependencies": {
    "add-to-homescreen": "^3.2.6",
    "awesomplete": "^1.0.0",
    "backbone": "^1.2.1",
    "backbone-pouch": "^1.4.0",
    "Base64": "^0.3.0",
    "bases": "^0.2.1",
    "bcrypt": "^3.0.3",
    "blob-util": "^1.1.2",
    "chokidar": "^1.2.0",
    "chokidar-cli": "^1.1.0",
    "coffeeify": "^3.0.1",
    "coffeescript": "^2.5.1",
    "cordova-plugin-qrscanner": "^2.6.0",
    "cordova-plugin-sqlite-2": "^1.0.5",
    "crypto-pouch": "^3.1.3",
    "cssnano": "^3.10.0",
    "datatables": "^1.10.7",
    "date-fns": "^2.16.1",
    "deep-object-diff": "^1.1.0",
    "file-saver": "^1.3.2",
    "filereader-stream": "^2.0.0",
    "get-form-data": "^2.0.0",
    "html-to-image": "^0.1.0",
    "jquery": "^3.3.1",
    "jquery.cookie": "^1.4.1",
    "js-cookie": "^2.0.0-pre",
    "jsqr": "^1.2.0",
    "jszip": "^3.0.0",
    "left-pad": "^1.3.0",
    "lodash.flatten": "^3.0.2",
    "lodash.merge": "^4.6.1",
    "material-design-icons-iconfont": "^6.1.0",
    "material-design-lite": "^1.3.0",
    "mdi": "^1.9.33",
    "memorystream": "^0.3.1",
    "mochify": "^2.13.0",
    "moment": "^2.10.3",
    "pouchdb": "^7.0.0",
    "pouchdb-adapter-cordova-sqlite": "^2.0.5",
    "pouchdb-adapter-http": "^7.2.2",
    "pouchdb-adapter-idb": "^7.2.2",
    "pouchdb-adapter-memory": "^7.2.1",
    "pouchdb-all-dbs": "^1.0.2",
    "pouchdb-core": "^7.2.2",
    "pouchdb-find": "^7.2.2",
    "pouchdb-mapreduce": "^7.2.2",
    "pouchdb-replication": "^7.2.2",
    "pouchdb-replication-stream": "^1.2.6",
    "pouchdb-upsert": "^2.2.0",
    "qrcode": "^1.3.3",
    "radix-64": "^1.0.4",
    "sha1": "^1.1.1",
    "simple-encryptor": "^2.0.0",
    "tabulator-tables": "^4.9.3",
    "uglifycss": "0.0.27",
    "underscore": "^1.8.3",
    "underscore.string": "^3.2.2"
  },
  "devDependencies": {
    "livereloadify": "^2.0.0",
    "node-static": "^0.7.7",
    "terser": "^3.16.1",
    "watchify": "^3.10.0"
  }
}
